# Think Tank Technologies Installation Scheduler Environment Variables
# Copy this file to .env and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Application Configuration
VITE_APP_NAME="Think Tank Technologies Installation Scheduler"
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development
VITE_BASE_PATH=/

# API Configuration
VITE_API_BASE_URL=http://localhost:3001/api
VITE_API_TIMEOUT=30000

# Google Services
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
VITE_GOOGLE_ANALYTICS_ID=your_google_analytics_id

# Email Services
VITE_SENDGRID_API_KEY=your_sendgrid_api_key
VITE_EMAIL_FROM_ADDRESS=noreply@yourdomain.com
VITE_EMAIL_FROM_NAME="Your Company Name"

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_HOTJAR=false
VITE_ENABLE_SENTRY=false

# Real-time Features
VITE_WEBSOCKET_URL=ws://localhost:3001/ws
REACT_APP_VAPID_PUBLIC_KEY=your-vapid-public-key

# PWA Features
VITE_APP_SHORT_NAME="TTT Scheduler"

# Development Tools
VITE_ENABLE_REACT_DEVTOOLS=true
VITE_ENABLE_REDUX_DEVTOOLS=true
VITE_SHOW_CONSOLE_LOGS=true

# Performance Settings
VITE_CHUNK_SIZE_WARNING_LIMIT=1000
VITE_ENABLE_BUNDLE_ANALYZER=false

# Security Settings
VITE_STRICT_CSP=false
VITE_ENABLE_HTTPS_ONLY=false

# Monitoring and Analytics (Optional)
VITE_SENTRY_DSN=your_sentry_dsn
VITE_HOTJAR_ID=your_hotjar_id
VITE_MIXPANEL_TOKEN=your_mixpanel_token

# Third-party Integrations (Optional)
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
VITE_INTERCOM_APP_ID=your_intercom_app_id

# CDN and Performance (Production)
VITE_CDN_URL=your_cdn_url
VITE_STATIC_ASSETS_URL=your_static_assets_url

# Cache Settings
VITE_CACHE_VERSION=1.0.0
VITE_SW_CACHE_NAME=your-app-cache-name