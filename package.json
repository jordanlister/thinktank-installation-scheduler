{
  "name": "installation-scheduler",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "dev:https": "vite --host 0.0.0.0 --port 3000 --https",
    "build": "vite build",
    "build:staging": "npm run type-check && vite build --mode staging",
    "build:production": "npm run type-check && vite build --mode production",
    "build:vercel": "vite build --mode production",
    "build:analyze": "npm run build && npm run analyze",
    "preview": "vite preview --host 0.0.0.0 --port 4173",
    "preview:staging": "vite preview --mode staging --host 0.0.0.0 --port 4173",
    "preview:production": "vite preview --mode production --host 0.0.0.0 --port 4173",
    "type-check": "npx tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:cache": "eslint . --ext .ts,.tsx,.js,.jsx --cache",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest run --reporter=verbose --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:staging": "playwright test --config playwright.staging.config.ts",
    "test:smoke:production": "playwright test --config playwright.smoke.config.ts",
    "test:a11y": "axe-playwright",
    "analyze": "vite-bundle-analyzer dist/stats.json",
    "lighthouse": "lhci autorun",
    "lighthouse:mobile": "lhci autorun --config lighthouserc.mobile.json",
    "db:migrate": "node scripts/db-migrate.js migrate",
    "db:seed": "node scripts/db-migrate.js seed",
    "db:setup": "node scripts/db-migrate.js setup",
    "db:reset": "node scripts/db-migrate.js reset",
    "db:check": "node scripts/db-migrate.js check",
    "db:backup": "./scripts/backup-restore.sh backup",
    "db:restore": "./scripts/backup-restore.sh restore",
    "data:generate": "node scripts/generate-test-data.js generate",
    "data:clean": "node scripts/generate-test-data.js clean",
    "setup": "./scripts/setup-dev.sh",
    "clean": "rm -rf dist coverage .eslintcache node_modules/.cache",
    "reset": "npm run clean && npm ci",
    "docker:build": "docker build -t ttt-scheduler .",
    "docker:build:dev": "docker build --target development -t ttt-scheduler:dev .",
    "docker:build:prod": "docker build --target production -t ttt-scheduler:prod .",
    "docker:run:dev": "docker-compose --profile development up",
    "docker:run:prod": "docker-compose --profile production up",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose down -v --rmi all",
    "vercel:deploy": "vercel --prod",
    "vercel:preview": "vercel",
    "vercel:env": "vercel env",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "security:check": "npm audit --audit-level high --json",
    "security:scan": "node scripts/security-audit.js",
    "security:headers": "node scripts/test-security-headers.js",
    "security:headers:dev": "node scripts/test-security-headers.js http://localhost:3000",
    "security:full": "npm run security:scan && npm run security:audit && npm run security:headers:dev",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "deps:update:major": "npx npm-check-updates -u",
    "prepare": "husky install || true",
    "pre-commit": "npm run lint:cache && npm run type-check && npm run test:run",
    "pre-push": "npm run build && npm run test:coverage"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@react-pdf/renderer": "^4.0.0",
    "@stripe/react-stripe-js": "^3.9.0",
    "@stripe/stripe-js": "^7.8.0",
    "@supabase/supabase-js": "^2.53.0",
    "@turf/turf": "^7.0.0",
    "@types/file-saver": "^2.0.7",
    "chart.js": "^4.4.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.0.0",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.23.12",
    "geokdbush": "^2.0.1",
    "geolib": "^3.3.4",
    "handlebars": "^4.7.8",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "leaflet": "^1.9.4",
    "leaflet-routing-machine": "^3.2.12",
    "leaflet.markercluster": "^1.5.3",
    "lucide-react": "^0.536.0",
    "nodemailer": "^6.9.15",
    "pdf-lib": "^1.17.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.62.0",
    "react-leaflet": "^4.2.1",
    "react-leaflet-cluster": "^3.1.0",
    "react-router-dom": "^6.26.1",
    "recharts": "^3.1.1",
    "stripe": "^18.4.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.15",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.40.1",
    "@sentry/react": "^7.89.0",
    "@sentry/vite-plugin": "^2.10.2",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/handlebars": "^4.1.0",
    "@types/html2canvas": "^1.0.0",
    "@types/leaflet": "^1.9.12",
    "@types/leaflet-routing-machine": "^3.2.8",
    "@types/leaflet.markercluster": "^1.5.4",
    "@types/nodemailer": "^6.4.15",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "axe-playwright": "^1.2.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^9.30.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "npm-check-updates": "^16.14.12",
    "postcss": "^8.5.6",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.16",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.6",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^3.2.4",
    "web-vitals": "^3.5.0"
  }
}
